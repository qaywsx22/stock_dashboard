{
  "manifest_version": 2,

  "name": "DACharts",
  "description": "This extension shows charts of user defined stocks.",
  "version": "0.1",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "img/Chart_Graph_32x28.png",
    "default_popup": "default_popup.html"
  },
  "permissions": [
    "https://*.dab-bank.de/*"
  ],
  "options_page": "options.html"
}
