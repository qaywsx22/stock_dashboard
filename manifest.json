{
  "manifest_version": 2,

  "name": "DACharts",
  "description": "This extension shows charts of user defined stocks.",
  "version": "0.5",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "img/Chart_Graph_32x28.png"
  },
  "permissions": [
    "https://*.dab-bank.de/*",
    "http://*.dab-bank.de/*",
	"tabs"
  ],
  "options_page": "options.html"
}
